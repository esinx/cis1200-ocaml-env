# DO NOT EDIT THIS FILE

# Set this to the the name of the top-level executable that should be executed:
MAIN := main

# autogenerated zipfile name with timestamp
ts := $(shell /bin/date "+%Y-%m-%d-%H:%M:%S")

.PHONY: all run clean zip

all:	$(MAIN).native

$(MAIN).native: main.ml assert.ml
	ocamlbuild -tag debug -pkg unix $(MAIN).native

run: $(MAIN).native
	OCAMLRUNPARAM=b ./$(MAIN).native

format: clean
clean:
	ocamlbuild -clean
	rm -rf *.zip

utop: 
	ocamlbuild -pkg unix assert.cmo